#!/usr/bin/python3
"""
    MyInt Class
"""


class MyInt(int):
    """
    class MyInt inerits from int
    """
"""
    MyInt that inherits from int
    """

    def __init__(self, num):
        """initialize num"""
        self.num = num

    def __eq__(self, other):
        """
        Return:
           True if both not equal
        """
        return self.num != other

    def __ne__(self, other):
        """
        Return:
           True if both equal
        """
        return self.num == other
